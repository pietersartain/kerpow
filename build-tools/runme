export CLASSPATH="/opt/Cloudscape10.0/lib/derby.jar:/opt/Cloudscape10.0/lib/derbytools.jar:${CLASSPATH}"

echo ""
if [ ! "$1" = "" ]; then
echo "##############"
echo "#  Running!  #"
echo "##############"
echo ""
java $1
echo ""
echo "######################"
echo "#  Program finished  #"
echo "######################"
else
echo "######################################"
echo "#  Please specify class file to run! #"
echo "######################################"
fi
echo ""
